  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "node-sass": "^3.6.0"
  },
  "scripts": {
    "clean": "rm -rf build && mkdir -p build",
    "static": "cp -r static/* build",
    "sass": "node-sass sass/style.scss --output build/css/",
