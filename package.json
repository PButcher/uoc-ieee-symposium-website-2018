    "prebuild": "npm run clean",
    "build": "npm run static && npm run sass && npm run js",
    "preserve": "npm run build",
    "live": "rsync -av build/ peter@pbutcher.uk:/var/www/researchsymposium/hosted/ieeeyp/"
  },
